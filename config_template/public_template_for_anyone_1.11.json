{
  "log": {
    "disabled": false,
    "level": "warn",
    "output": "sing-box.log",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "secret": "",
      "default_mode": "rule",
      "access_control_allow_origin": "https://board.zash.run.place",
      "access_control_allow_private_network": true
    },
    "cache_file": {
      "enabled": true,
      "store_rdrc": true
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "address": "https:\/\/1.1.1.1\/dns-query",
        "detour": "✈️ Proxy"
      },
      {
        "tag": "dns_direct",
        "address": "https:\/\/223.5.5.5\/dns-query",
        "detour": "Direct"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "dns_direct"
      },
      {
        "clash_mode": "direct",
        "server": "dns_direct"
      },
      {
        "clash_mode": "global",
        "server": "dns_proxy"
      },
      {
        "rule_set": [
          "geosite-dnsblock",
          "geosite-adguard"
        ],
        "action": "reject",
        "method": "default"
      },
      {
        "domain": [
          "p2p.qq.com",
          "p2ptun.qq.com",
          "p2pupdate.gamedl.qq.com",
          "pcdn.yximgs.com",
          "p2p.douyin.com",
          "onethingpcs.com",
          "kuiniuca.com"
        ],
        "domain_suffix": [
          "uhabo.com",
          "xycdn.com"
        ],
        "action": "reject",
        "method": "default"
      },
      {
        "network": "tcp",
        "port": 853,
        "action": "reject",
        "method": "default"
      },
      {
        "network": "udp",
        "port": 443,
        "action": "reject",
        "method": "default"
      },
      {
        "domain_suffix": [
          "icloudnative.io",
          "fuckcloudnative.io",
          "sealos.io",
          "cdn.jsdelivr.net",
          "emby.wtf",
          "term.wtf",
          "makima.online",
          "zxxk.com",
          "xkw.com"
        ],
        "ip_is_private": true,
        "rule_set": [
          "geosite-tencent",
          "geosite-bilibili",
          "geosite-geolocation-cn"
        ],
        "server": "dns_direct"
      },
      {
        "query_type": "CNAME",
        "domain_suffix": [
          "synn.cc",
          "ping0.cc",
          "aca.best",
          "acaisbest.com",
          "122777.xyz",
          "emby.aca.best",
          "misakaf.org",
          "nanflix.net"
        ],
        "ip_cidr": [
          "91.108.16.0/22",
          "91.108.56.0/22"
        ],
        "rule_set": [
          "geosite-twitter",
          "geosite-youtube",
          "geosite-netflix",
          "geosite-geolocation-!cn",
          "geosite-telegram",
          "geosite-openai",
          "geosite-google",
          "geosite-github"
        ],
        "server": "dns_proxy"
      }
    ],
    "strategy": "prefer_ipv4",
    "disable_cache": false,
    "disable_expire": false,
    "independent_cache": false,
    "final": "dns_proxy"
  },
  "ntp": {
    "enabled": true,
    "interval": "30m0s",
    "server": "time.apple.com",
    "server_port": 123,
    "detour": "Direct"
  },
  "inbounds": [
    {
      "type": "socks",
      "listen": "::",
      "listen_port": 7890
    },
    {
            "type": "tun",
            "tag": "tun-in",
            "mtu": 9000,
            "interface_name": "tun.sing-box",
            "address": [
              "172.18.0.1/30",
              "fdfe:dcba:9876::1/126"
            ],
            "auto_route": true,
            "strict_route": true,
            "route_address": [
              "0.0.0.0/1",
              "128.0.0.0/1",
              "::/1",
              "8000::/1"
            ],
            "route_exclude_address": [
              "192.168.0.0/16",
              "fc00::/7"
            ],
            "sniff": true,
            "stack": "system"
        }
  ],
  "outbounds": [
    {
      "tag": "✈️ Proxy",
      "type": "selector",
      "outbounds": [
        "👉 Select",
        "🇺🇸 US",
        "🇯🇵 JP",
        "🇨🇳 TW",
        "🇭🇰 HK",
        "🇸🇬 SG"
      ]
    },
    {
      "type": "selector",
      "tag": "openai",
      "outbounds": [
        "👉 Select",
        "🇺🇸 US"
      ],
      "default": "🇺🇸 US"
    },
    {
      "type": "selector",
      "tag": "Telegram",
      "outbounds": [
        "👉 Select",
        "🇸🇬 SG"
      ],
      "default": "🇸🇬 SG"
    },
    {
      "type": "selector",
      "tag": "youtube",
      "outbounds": [
        "👉 Select",
        "🇺🇸 US",
        "🇯🇵 JP",
        "🇨🇳 TW",
        "🇭🇰 HK",
        "🇸🇬 SG"
      ],
      "default": "🇸🇬 SG"
    },
    {
      "type": "selector",
      "tag": "Emby",
      "outbounds": [
        "Direct",
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "剩余|距离|到期"
          ]
        }
      ]
    },
    {
      "type": "selector",
      "tag": "👉 Select",
      "outbounds": [
        "{all}"
      ]
    },
    {
      "tag": "Microsoft",
      "type": "selector",
      "outbounds": [
        "Direct",
        "✈️ Proxy",
        "🇺🇸 US"
      ]
    },
    {
      "tag": "🇭🇰 HK",
      "type": "urltest",
      "url": "http://www.gstatic.com/generate_204",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇭🇰|HK|hk|香港|港|HongKong"
          ]
        }
      ]
    },
    {
      "type": "urltest",
      "tag": "🇨🇳 TW",
      "url": "http://www.gstatic.com/generate_204",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇨🇳|台湾|TW|TAIWAN|Taiwan"
          ]
        }
      ]
    },
    {
      "type": "urltest",
      "tag": "🇯🇵 JP",
      "url": "http://www.gstatic.com/generate_204",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇯🇵|日本|JP|JAPAN|Japan"
          ]
        }
      ]
    },
    {
      "type": "urltest",
      "tag": "🇸🇬 SG",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇸🇬|新加坡|SG|SINGAPORE|Singapore"
          ]
        }
      ]
    },
    {
      "type": "urltest",
      "tag": "🇺🇸 US",
      "url": "http://www.gstatic.com/generate_204",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇺🇸|美国|US|UNITED STATES|United States"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "Austria|Russia"
          ]
        }
      ]
    },
    {
      "type": "direct",
      "tag": "Direct"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "clash_mode": "direct",
        "outbound": "Direct"
      },
      {
        "clash_mode": "global",
        "outbound": "✈️ Proxy"
      },
      {
        "ip_cidr": [
          "91.108.16.0/22",
          "91.108.56.0/22"
        ],
        "rule_set": [
          "geosite-telegram"
        ],
        "outbound": "Telegram"
      },
      {
        "domain_suffix": [
          "emby.aca.best",
          "misakaf.org",
          "nanflix.net"
        ],
        "outbound": "Emby"
      },
      {
        "rule_set": [
          "geosite-openai"
        ],
        "outbound": "openai"
      },
      {
        "rule_set": [
          "geosite-youtube"
        ],
        "outbound": "youtube"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "domain_suffix": [
              "synn.cc",
              "ping0.cc",
              "aca.best",
              "acaisbest.com",
              "122777.xyz"
            ]
          },
          {
            "rule_set": [
              "geosite-twitter",
              "geosite-google",
              "geosite-github",
              "geosite-netflix",
              "geosite-geolocation-!cn"
            ]
          }
        ],
        "action": "route",
        "outbound": "✈️ Proxy"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "domain_suffix": [
              "icloudnative.io",
              "fuckcloudnative.io",
              "sealos.io",
              "cdn.jsdelivr.net",
              "emby.wtf",
              "term.wtf",
              "makima.online",
              "zxxk.com",
              "xkw.com"
            ]
          },
          {
            "ip_is_private": true
          },
          {
            "rule_set": [
              "geosite-tencent",
              "geosite-bilibili",
              "geosite-geolocation-cn"
            ]
          }
        ],
        "action": "route",
        "outbound": "Direct"
      }
    ],
    "rule_set": [
            {
                "type": "remote",
                "tag": "geosite-twitter",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/Twitter/Twitter.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-geolocation-cn",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/China/China.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-geolocation-!cn",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/Global/Global.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-google",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/Google/Google.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-github",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/GitHub/GitHub.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-youtube",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/YouTube/YouTube.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-openai",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/OpenAI/OpenAI.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-telegram",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/Telegram/Telegram.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-netflix",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/Netflix/Netflix.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-tencent",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/Tencent/Tencent.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-bilibili",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/BiliBili/BiliBili.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-dnsblock",
                "format": "binary",
                "url": "https://github.com/TG-Twilight/AWAvenue-Ads-Rule/raw/refs/heads/main/Filters/AWAvenue-Ads-Rule-Singbox.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-adguard",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/senshinya/singbox_ruleset@main/rule/AdGuardSDNSFilter/AdGuardSDNSFilter.srs"
            }
        ],
    "final": "✈️ Proxy",
    "find_process": true,
    "auto_detect_interface": true
  }
}
